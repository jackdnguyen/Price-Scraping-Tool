    <div class='filter'>
        <header class="Atitle">Sku Search</header>
        <form action="skuwSearch" method="post">
          <input name="Sku_name" id="Sku_name" class="filtersku" type="text" placeholder="SKU Number">
          <input class="filtersubmit" type="submit">
        </form>
       
        
      </div>
    <div class="container">
      <form action="/company-name" method="POST">
        <!-- <h2>Canadian Appliance</h2> -->
      </form>

      <table id="displayData">
        <tr>
          <th>No.</th>
          <th>SKU</th>
          <th>Product's Name</th>
          <th>Sales Price</th>
          <th>Url</th>
          <th>Last Price Updated</th>
        </tr>
        <form id="display-data" action="/display-data" method="POST">
          <% results.forEach(function(r) { %>
          <tr>
              <td><%=r.id%></td>
              <td><%=r.sku%></td>
              <td><%=r.name%></td>
              <td><%=r.price%></td>
              <td><%=r.url%></td>
              <td><%=r.lpmod%></td>
            </form>
          </tr>
          <% }); %>
      </form>

